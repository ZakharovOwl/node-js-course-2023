    "@types/morgan": "^1.9.6",
    "@types/node": "^20.5.7",
    "@types/pg": "^8.10.5",
    "@types/uuid": "^9.0.3",
    "bcryptjs": "^2.4.3",
    "debug": "^4.3.4",
    "dotenv": "^16.3.1",
    "husky": "^8.0.3",
    "jest": "^29.6.4",
    "jsonwebtoken": "^9.0.2",
    "lint-staged": "^15.0.2",
    "morgan": "^1.10.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "uuid": "^9.0.0",
    "winston": "^3.11.0"
