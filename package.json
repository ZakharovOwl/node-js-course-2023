    "body-parser": "^1.20.2",
    "csvtojson": "^2.0.10",
    "express": "^4.18.2",
    "joi": "^17.10.1",
    "mongoose": "^7.5.3",
    "pg": "^8.11.3",
    "pg-promise": "^11.5.4",
    "prettier": "^3.0.3",
    "uuid": "^9.0.0"
  },
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "./src/config//mikro-orm.config.ts",
